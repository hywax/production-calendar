{
  "name": "production-calendar",
  "version": "0.2.0",
  "packageManager": "pnpm@9.0.6",
  "description": "",
  "author": {
    "name": "hywax",
    "url": "https://hywax.space/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hywax/production-calendar.git"
  },
  "exports": {
    "./sources/*": {
      "types": "./sources/*.d.ts",
      "import": "./sources/*.mjs",
      "require": "./sources/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "production-calendar": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "sources"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --fix",
    "test:types": "tsc --noEmit --skipLibCheck",
    "release": "pnpm build && changelogen --release --push && pnpm publish",
    "production-calendar": "pnpm jiti src/cli",
    "prepare": "husky"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20.12.8",
    "changelogen": "^0.5.5",
    "husky": "^9.0.11",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  },
  "lint-staged": {
    "*.ts": "pnpm lint"
  }
}
