{
  "name": "production-calendar",
  "version": "0.5.3",
  "packageManager": "pnpm@10.29.1",
  "description": "",
  "author": {
    "name": "hywax",
    "url": "https://hywax.space/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hywax/production-calendar.git"
  },
  "exports": {
    "./sources/*": {
      "types": "./sources/*.d.ts",
      "import": "./sources/*.mjs",
      "require": "./sources/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "production-calendar": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "sources"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --fix",
    "test:types": "tsc --noEmit --skipLibCheck",
    "release": "pnpm build && changelogen --release --push && pnpm publish",
    "production-calendar": "pnpm jiti src/cli",
    "generate": "pnpm jiti src/generator",
    "prepare": "husky"
  },
  "dependencies": {
    "citty": "^0.2.0",
    "consola": "^3.4.2",
    "fast-xml-parser": "^5.3.4",
    "ics": "^3.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/node": "^24.10.11",
    "changelogen": "^0.6.2",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*.ts": "pnpm lint"
  }
}
