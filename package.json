{
  "name": "production-calendar",
  "version": "0.5.1",
  "packageManager": "pnpm@9.7.0",
  "description": "",
  "author": {
    "name": "hywax",
    "url": "https://hywax.space/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hywax/production-calendar.git"
  },
  "exports": {
    "./sources/*": {
      "types": "./sources/*.d.ts",
      "import": "./sources/*.mjs",
      "require": "./sources/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "production-calendar": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "sources"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint . --fix",
    "test:types": "tsc --noEmit --skipLibCheck",
    "release": "pnpm build && changelogen --release --push && pnpm publish",
    "production-calendar": "pnpm jiti src/cli",
    "generate": "pnpm jiti src/generator",
    "prepare": "husky"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "fast-xml-parser": "^4.4.1",
    "ics": "^3.7.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20.14.15",
    "changelogen": "^0.5.5",
    "husky": "^9.1.4",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.8",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0"
  },
  "lint-staged": {
    "*.ts": "pnpm lint"
  }
}
